{
  "name": "fleabag-quotes",
  "version": "1.4.0",
  "url": "httpsgp://fleabag-quotes.livkndt.com/",
  "description": "Fleabag quotes API. Get random quotes from Phoebe Waller-Bridge's Fleabag.",
  "repository": {
    "type": "git",
    "url": "https://github.com/livkndt/fleabag-quotes"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "ts-node src/index.ts",
    "serve": "node dist/index.js",
    "postinstall": "tsc",
    "build": "rimraf dist && tsc",
    "add-build": "git add dist",
    "ts-check": "tsc --project tsconfig.json",
    "dev": "nodemon",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky install",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "swagger-autogen": "npm run build && node ./dist/swagger/swagger.js",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "20.x"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add ."
    ]
  },
  "pre-commit": [
    "ts-check",
    "build",
    "add-build"
  ],
  "keywords": [
    "fleabag",
    "quotes",
    "phoebe waller-bridge"
  ],
  "author": "Olivia Knoedt",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.11.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.49.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
