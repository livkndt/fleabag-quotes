{
  "name": "fleabag-quotes",
  "version": "1.4.0",
  "description": "Fleabag quotes API. Get random quotes from Phoebe Waller-Bridge's Fleabag.",
  "repository": {
    "type": "git",
    "url": "https://github.com/livkndt/fleabag-quotes"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/server.js",
    "postinstall": "tsc",
    "build": "tsc",
    "dev": "nodemon",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "format": "prettier --write .",
    "swagger-autogen": "npm run build && node ./dist/swagger/swagger.js"
  },
  "engines": {
    "node": "18.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\" npm run swagger-autogen && git add ./src/swagger_output.json"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": "prettier --write"
  },
  "keywords": [
    "fleabag",
    "quotes",
    "phoebe waller-bridge"
  ],
  "author": "Olivia Knoedt",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.11.2",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
