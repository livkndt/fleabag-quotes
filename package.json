{
  "name": "fleabag-quotes",
  "version": "1.4.0",
  "url": "https://fleabag-quotes.livkndt.com/",
  "description": "Fleabag quotes API. Get random quotes from Phoebe Waller-Bridge's Fleabag.",
  "repository": {
    "type": "git",
    "url": "https://github.com/livkndt/fleabag-quotes"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "ts-node src/index.ts",
    "build": "rimraf dist && tsc",
    "serve": "node dist/index.js",
    "dev": "nodemon",
    "postinstall": "tsc",
    "ts-check": "tsc --project tsconfig.json",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "swagger-autogen": "npm run build && node ./dist/swagger/swagger.js",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "24.x"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add ."
    ]
  },
  "keywords": [
    "fleabag",
    "quotes",
    "phoebe waller-bridge"
  ],
  "author": "Olivia Knoedt",
  "license": "MIT",
  "dependencies": {
    "canvas": "^3.0.0",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "swagger-ui-express": "=4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.14",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.14",
    "prettier": "^3.8.1",
    "rimraf": "^5.0.10",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
